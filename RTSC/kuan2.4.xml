	<template>
		<name>Field</name>
		<declaration>int i;
int index;
int j;

void dequeue()
{
	while(bales[0] == bales[0+1])
	{
		//int i = 0;
        i = 0;
		while (i &lt; len &amp;&amp; i &lt; 14)
		{
			bales[i] = bales[i+1];
			i++;
		}
		bales[i] = 0;
	}
	
//	int i = 0;
    i = 0;
	len -= 1;
	while (i &lt; len)
	{
		bales[i] = bales[i+1];
		i++;
	}
	bales[i] = 0;
}

void enqueue(int element, int number)
{
	index = -1;
	for (i = 0; bales[i] != 0; i++)
	{
		index = i;
	}
	index++;

	for (j = 0; j &lt; number; j++)
	{
		bales[index] = element;
		index++;
	}
}</declaration>
