<nta>
	<declaration>// Place global declarations here.
chan park, unpark, move, end_of_street, continue_on_street;

bool first_car_parked = false;
bool second_car_parked = false;</declaration>
	<template>
		<name x="5" y="5">Car</name>
		<declaration>// Place local declarations here.
clock wait;
int position = 0;
bool parked = false;</declaration>
		<location id="id0" x="8" y="34">
			<name x="-2" y="0">start</name>
		</location>
		<location id="id1" x="246" y="34">
			<name x="195" y="51">find_parking_spot</name>
		</location>
		<location id="id2" x="535" y="34">
			<name x="552" y="17">perform_parking</name>
		</location>
		<location id="id3" x="85" y="246">
			<name x="42" y="255">exit_street</name>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id1"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="110" y="136">end_of_street!</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="guard" x="340" y="102">wait &gt; 5</label>
			<label kind="synchronisation" x="357" y="127">unpark!</label>
			<label kind="assignment" x="374" y="153">parked = false</label>
			<nail x="535" y="246"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="399" y="8">park!</label>
			<label kind="assignment" x="391" y="34">position++, 
parked = true, 
wait = 0</label>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id1"/>
			<label kind="guard" x="85" y="-119">first_car_parked == false || second_car_parked == false</label>
			<label kind="synchronisation" x="178" y="-144">continue_on_street!</label>
			<label kind="assignment" x="212" y="-102">position++</label>
			<nail x="340" y="-119"/>
			<nail x="144" y="-119"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="102" y="8">move!</label>
			<label kind="assignment" x="85" y="34">position++</label>
		</transition>
	</template>
	<template>
		<name>Lane</name>
		<declaration>int street_length = 0;</declaration>
		<location id="id4" x="-459" y="42">
			<name x="-510" y="59">street_beginning</name>
		</location>
		<location id="id5" x="-323" y="42">
			<name x="-382" y="59">find_parking_spot_one</name>
		</location>
		<location id="id6" x="-119" y="42">
			<name x="-145" y="59">continue</name>
		</location>
		<location id="id7" x="76" y="42">
			<name x="8" y="59">find_parking_spot_two</name>
		</location>
		<location id="id8" x="289" y="42">
			<name x="246" y="59">street_continue</name>
		</location>
		<location id="id9" x="459" y="42">
			<name x="425" y="59">street_end</name>
		</location>
		<location id="id10" x="-323" y="-221">
			<name x="-374" y="-263">parking_spot_one</name>
		</location>
		<location id="id11" x="76" y="-212">
			<name x="25" y="-263">parking_spot_two</name>
		</location>
		<init ref="id4"/>
		<transition>
			<source ref="id8"/>
			<target ref="id9"/>
			<label kind="synchronisation" x="323" y="17">end_of_street?</label>
			<label kind="assignment" x="314" y="42">street_length++</label>
		</transition>
		<transition>
			<source ref="id7"/>
			<target ref="id8"/>
			<label kind="guard" x="93" y="17">second_car_parked == true</label>
			<label kind="assignment" x="136" y="42">street_length++</label>
		</transition>
		<transition>
			<source ref="id11"/>
			<target ref="id8"/>
			<label kind="guard" x="178" y="-127">second_car_parked == true</label>
			<label kind="synchronisation" x="195" y="-102">unpark?</label>
			<label kind="assignment" x="221" y="-85">second_car_parked = false,
street_length++</label>
		</transition>
		<transition>
			<source ref="id7"/>
			<target ref="id11"/>
			<label kind="guard" x="-42" y="-119">second_car_parked == false</label>
			<label kind="synchronisation" x="25" y="-102">park?</label>
			<label kind="assignment" x="-25" y="-85">second_car_parked = true</label>
		</transition>
		<transition>
			<source ref="id6"/>
			<target ref="id7"/>
			<label kind="synchronisation" x="-85" y="17">continue_on_street?</label>
			<label kind="assignment" x="-85" y="42">street_length++</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id6"/>
			<label kind="guard" x="-306" y="17">first_car_parked == true</label>
			<label kind="assignment" x="-263" y="42">street_length++</label>
		</transition>
		<transition>
			<source ref="id10"/>
			<target ref="id6"/>
			<label kind="guard" x="-254" y="-127">first_car_parked == true</label>
			<label kind="synchronisation" x="-203" y="-110">unpark?</label>
			<label kind="assignment" x="-238" y="-93">first_car_parked = false,
street_length++</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id10"/>
			<label kind="guard" x="-442" y="-119">first_car_parked == false</label>
			<label kind="synchronisation" x="-382" y="-102">park?</label>
			<label kind="assignment" x="-425" y="-85">first_car_parked = true</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id5"/>
			<label kind="synchronisation" x="-416" y="17">move?</label>
			<label kind="assignment" x="-441" y="42">street_length++</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
carOne = Car();
carTwo = Car();
rLane = Lane();
lLane = Lane();

// List one or more processes to be composed into a system.
system carOne, carTwo, rLane, lLane;
    </system>
